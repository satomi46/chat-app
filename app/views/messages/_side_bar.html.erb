
    <div class="title-logo">
      <div class="user-name">
        <%= link_to current_user.name, edit_user_path(current_user)%>
      </div>
      <div class="new-chat-btn">
        <%= link_to 'チャットを作成する', new_room_path %>
      </div>
    </div>
    <div class="chat-list">
      <% current_user.rooms.each do |room| %>
        <div class="chatroom">
          <a href="#"><%= room.name %></a>
        </div>
      <% end %>
    </div>